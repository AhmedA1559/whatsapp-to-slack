# Slack Incoming Webhook URL
# Get this from: Slack App > Incoming Webhooks > Add New Webhook to Workspace
SLACK_WEBHOOK_URL="https://hooks.slack.com/services/YOUR/WEBHOOK/URL"

# Vonage AI Studio API Key
# Get this from: AI Studio > Click user icon > Generate API Key
AI_STUDIO_KEY="your-ai-studio-api-key"

# AI Studio Region (eu or us)
AI_STUDIO_REGION="eu"

# Server Port (optional, defaults to 3000)
PORT=3000
